<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="car" class="com.bi.beans.Car">
		<constructor-arg name="name" value="baleno" />
		<constructor-arg name="color" value="white" />
		<constructor-arg name="company" value="maruti suzuki" />
		<constructor-arg name="carType" value="diesel" />
		<constructor-arg name="price" value="600000" />
	</bean>
