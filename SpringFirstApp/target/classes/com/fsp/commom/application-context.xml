<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="messagewriter" class="com.fsp.beans.MessageWriter">
		<property name="messageconverter"</property>
	</bean>
	<bean id="htmlmessagewriter" class="com.fsp.beans.HtmlMessageWriterImpl" />
	<bean id="pdfmessagewriter" class="com.fsp.beans.PdfMessageWriterImpl" />

</beans>
